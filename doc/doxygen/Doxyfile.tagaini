# extract private, package and static info
EXTRACT_PRIVATE        = NO
EXTRACT_PACKAGE        = NO
EXTRACT_STATIC         = NO

# avoid limiting the fields shown in the diagrams
UML_LIMIT_NUM_FIELDS   = 0