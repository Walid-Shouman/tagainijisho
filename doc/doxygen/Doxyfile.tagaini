# Basic Parameters
PROJECT_NAME           = "Tagaini Jisho"

RECURSIVE              = YES

EXCLUDE_PATTERNS       = .git \
                         venv \
                         *.py

GENERATE_XML           = YES

# Generate UML diagrams parameters

EXTRACT_ALL            = YES
HAVE_DOT               = YES
